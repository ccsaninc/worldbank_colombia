    dis "---------------------------------------------------------------------------------------------------------------"
	
	dis "-------------------------------------"
    dis "GEIH_`i' - Fuente: TOTAL NACIONAL"
    dis "-------------------------------------"
	
		tab mes `var' [iw=fex_c_2011]
		tab mes sexo if `var'==1 [iw=fex_c_2011] 
		tab mes g_etarios if `var'== 1 [iw=fex_c_2011]
		
	dis "---------------------------------------------------------------------------------------------------------------"
	
	dis "-------------------------------------"
    dis "GEIH_`i' - Fuente: URBANO"
    dis "-------------------------------------"
	
		tab mes `var' if fuente =="Cabecera" [iw=fex_c_2011]
		tab mes sexo if `var'==1 & fuente =="Cabecera" [iw=fex_c_2011] 
		tab mes g_etarios if `var'== 1 & fuente =="Cabecera" [iw=fex_c_2011]
		
	dis "---------------------------------------------------------------------------------------------------------------"
	
	dis "-------------------------------------"
    dis "GEIH_`i' - Fuente: 13 AM"
    dis "-------------------------------------"
	
		tab mes `var' if ciudad13 == 1 [iw=fex_c_2011]
		tab mes sexo if `var'==1 & ciudad13 == 1 [iw=fex_c_2011] 
		tab mes g_etarios if `var'== 1 & ciudad13 == 1 [iw=fex_c_2011]
		
	dis "---------------------------------------------------------------------------------------------------------------"
	
	dis "-------------------------------------"
    dis "GEIH_`i'  - Fuente: 23 AM"
    dis "-------------------------------------"
	
		tab mes `var' if ciudad23 == 1 [iw=fex_c_2011]
		tab mes sexo if `var'==1 & ciudad23 == 1 [iw=fex_c_2011] 
		tab mes g_etarios if `var'== 1 & ciudad23 == 1 [iw=fex_c_2011]
		
	dis "---------------------------------------------------------------------------------------------------------------"
	
	dis "-------------------------------------"
    dis "GEIH_`i'  - Fuente: RURAL"
    dis "-------------------------------------"
	
		tab mes `var' if fuente =="Resto" [iw=fex_c_2011]
		tab mes sexo if `var'==1 & fuente =="Resto" [iw=fex_c_2011] 
		tab mes g_etarios if `var'== 1 & fuente =="Resto" [iw=fex_c_2011]




		*- Identificacion de las 23 Ciudades 
				
		generate ciudad23 = 1 if area_2 == 5 | area_2==8 | area_2==11 | area_2==13 | area_2==15 | area_2==17 | area_2==18 | area_2==19 | area_2==20 ///
		| area_2==23 | area_2==27 | area_2==41 | area_2==44 | area_2==47 | area_2==50 | area_2==52 | area_2==54 | area_2==63 | area_2==66 | area_2==68 ///
		| area_2==70 | area_2==73 | area_2==76 |area_2==81 | area_2==85 | area_2==86 | area_2==88 | area_2==91 | area_2==94 | area_2==95 | area_2==97 ///
		| area_2==99
				
		replace ciudad23 = 0 if ciudad23 != 1
		label var ciudad23 "23 ciudades"				
				
		*- Dummy de 10 ciuades
				
		gen ciudad10 = 1 if area_2==15 | area_2==18 | area_2==19 | area_2==20 | area_2==27 | ///
		area_2==41 | area_2==44 | area_2==47 | area_2==63 | area_2==70
		
		replace ciudad10=0 if ciudad10!=1
		label var ciudad10 "10 ciudades"
			
		*- Dummy Trece Areas 
				
		gen ciudad13=1 if area_2==5 | area_2==8 | area_2==11 | area_2==13 | area_2==17 | ///
		area_2==23 | area_2==50 | area_2==52 | area_2==54 | area_2==66 | area_2==68 | area_2==73 | area_2==76
		replace ciudad13=0 if ciudad13!=1
		label var ciudad13 "Areas Metropolitanas"
				
		*- Dummy Bogota (Capital)
				
		gen capital=1 if area_2==11 
		replace capital=0 if capital!=1
		label var capital "Bgot√° D.C."
				
		*- Dummy Resto
				
		gen resto=1 if clase=="2"
		replace resto=0 if clase=="1"
		label var resto "Resto de las zonas del pais"
				
		*- Dummy Otas Cabeceras (La resta del total de Cabeceras, menos las 23 principales ciudades)
				
		gen otras_cabeceras = 1 if clase == "1" & ciudad23 !=1	
		replace otras_cabeceras = 0 if otras_cabeceras != 1
		label var otras_cabeceras "Otras Cabeceras - DANE"



































